   "source": [
    "#pos extract\n",
    "tok_sen = []\n",
    "for i in sent:\n",
    "    tok_sen.append(nltk.word_tokenize(i))\n",
    "tok_sen2 = []\n",
    "for i in tok_sen:\n",
    "    tok_sen2.append(nltk.pos_tag(i,tagset='universal'))\n",
    "tok_sen3 = []\n",
    "for i in tok_sen2:\n",
    "    for j in i:\n",
    "        tok_sen3.append(j)\n",
    "        \n",
    "\n",
    "stopword = ['’','s','“','”']\n",
    "def pos(text,pos):\n",
    "    word = []\n",
    "    for i in text:\n",
    "        if i[1] == pos and i[0] not in stopword:\n",
    "            word.append((i[0]))\n",
    "    return word\n",
    "\n",
    "pos(tok_sen3,'NOUN')"
   ]
  },
