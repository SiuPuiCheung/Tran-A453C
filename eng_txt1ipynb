    "#Sentence length\n",
    "sent_length = {}\n",
    "for i in sent:\n",
    "    word = i.split()\n",
    "    sent_length[i] = len(word)\n",
    "sldf = pd.DataFrame(sent_length, index=[0]).T\n",
    "sldf = sldf.sort_values(by=0, ascending=False)\n",
